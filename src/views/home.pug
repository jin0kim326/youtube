extends layouts/main
include mixins/videoBlock

block content
    .home-videos
        each item in videos
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl,
                creatorAvatarUrl:item.creator.avatarUrl,
                creatorName:item.creator.name,
                uploadTime:item.uploadTime
            })